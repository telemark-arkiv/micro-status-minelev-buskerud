{
  "name": "micro-status-minelev-buskerud",
  "description": "Microservice for tilskudd status",
  "version": "1.0.26",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.4.0"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "NODE_ENV=production micro -p 8080",
    "dev": "micro",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "status",
    "MinElev",
    "Buskerud fylkeskommune",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/telemark/micro-status-minelev-buskerud.git"
  },
  "bugs": {
    "url": "http://github.com/telemarks/micro-status-minelev-buskerud/issues"
  },
  "now": {
    "alias": "bfk.minelev.status.t-fk.win"
  },
  "greenkeeper": {
    "emails": false
  },
  "devDependencies": {
    "coveralls": "3.0.0",
    "nsp": "2.6.3",
    "standard": "8.6.0",
    "tap": "11.0.0"
  },
  "dependencies": {
    "got": "8.0.0",
    "marked": "0.3.6",
    "micro": "9.0.2"
  }
}